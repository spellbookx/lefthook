#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dcdavidev/lefthook/node_modules/.pnpm/conventional-recommended-bump@11.2.0/node_modules/conventional-recommended-bump/dist/cli/node_modules:/home/dcdavidev/lefthook/node_modules/.pnpm/conventional-recommended-bump@11.2.0/node_modules/conventional-recommended-bump/dist/node_modules:/home/dcdavidev/lefthook/node_modules/.pnpm/conventional-recommended-bump@11.2.0/node_modules/conventional-recommended-bump/node_modules:/home/dcdavidev/lefthook/node_modules/.pnpm/conventional-recommended-bump@11.2.0/node_modules:/home/dcdavidev/lefthook/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dcdavidev/lefthook/node_modules/.pnpm/conventional-recommended-bump@11.2.0/node_modules/conventional-recommended-bump/dist/cli/node_modules:/home/dcdavidev/lefthook/node_modules/.pnpm/conventional-recommended-bump@11.2.0/node_modules/conventional-recommended-bump/dist/node_modules:/home/dcdavidev/lefthook/node_modules/.pnpm/conventional-recommended-bump@11.2.0/node_modules/conventional-recommended-bump/node_modules:/home/dcdavidev/lefthook/node_modules/.pnpm/conventional-recommended-bump@11.2.0/node_modules:/home/dcdavidev/lefthook/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../conventional-recommended-bump/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../conventional-recommended-bump/dist/cli/index.js" "$@"
fi
